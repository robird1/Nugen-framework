<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="st_button_basic">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginEnd">@dimen/dim_button_default_margin_right</item>
        <item name="android:layout_marginStart">@dimen/dim_button_default_margin_left</item>
        <item name="android:minHeight">51dp</item>
    </style>


<style name="st_button_basic_no_center">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginEnd">@dimen/dim_button_default_margin_right</item>
        <item name="android:layout_marginStart">@dimen/dim_button_default_margin_left</item>
        <item name="android:minHeight">51dp</item>
    </style>

    <style name="st_button_vertical_default_margin" parent="st_button_basic">
        <item name="android:paddingStart">@dimen/dim_button_horizontal_padding</item>
        <item name="android:paddingEnd">@dimen/dim_button_horizontal_padding</item>
        <item name="android:orientation">vertical</item>
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/bg_white_to_brown_click</item>
    </style>
    
