<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <style name="Theme.Size50" parent="AppBaseTheme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:actionBarSize">50dip</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowFullscreen">true</item>
		<!--Support Library -->
        <item name="windowActionBar">false</item>
		<item name="actionBarSize">50dip</item>
<!--         <item name="android:windowAnimationStyle">@style/ActivityAnimation</item> -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="Theme.Size50">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    
<style name="ActivityAnimation" parent="@android:style/Animation.Activity">
    <item name="android:activityOpenEnterAnimation">@anim/swipe_left_in</item>
    <item name="android:activityOpenExitAnimation">@anim/swipe_left_out</item>
    <item name="android:activityCloseEnterAnimation">@anim/swipe_right_in</item>
    <item name="android:activityCloseExitAnimation">@anim/swipe_right_out</item>
</style>
    
    
