<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background" >

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:gravity="center"
        android:text="Temp screen (not in final version)"
        android:textSize="50dp" />

    <Button
        android:id="@+id/id_continue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="72dp"
        android:onClick="setup_click"
        android:text="Continue" />

    <Button
        android:id="@+id/id_gototmain"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/id_continue"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="26dp"
        android:onClick="to_main_click"
        android:text="Skip to main" />

</RelativeLayout>
