<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:focusable="false"
    android:clickable="false"
     >
    
    <LinearLayout
        android:id="@+id/id_button_b32_seperate"
        style="@style/st_button_vertical_default_margin_16px"
        android:layout_height="26dp"
        android:background="@color/light_brown"
    	android:focusable="false"
    	android:clickable="false">

        <com.accu_chek.solo_m.rcapp.application.gui.ui.FontManager
            android:id="@+id/id_b28_text"
            style="@style/st_font_SmallTitle_bold"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:gravity="center_vertical"
            android:lines="1"
            android:text="Forgot text?"
	    	android:focusable="false"
    		android:clickable="false"
            android:textColor="@color/main_grey" />
    </LinearLayout>
    
    <LinearLayout
        android:id="@+id/id_button_b32_main"
        android:layout_width="match_parent"
		android:layout_height="51dp"
		android:layout_marginEnd="16dp"
	    	android:focusable="false"
    		android:clickable="false"
		android:layout_marginStart="16dp" >

            <com.accu_chek.solo_m.rcapp.application.gui.ui.FontManager
                android:id="@+id/id_b32_title"
                style="@style/st_font_SmallTitle"
                android:layout_width="70dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:maxLines="2" />
                        <LinearLayout
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:layout_marginStart="2dp"
            android:orientation="vertical"
            android:focusable="false"
    		android:clickable="false"
            android:paddingTop="3dp" >

            <ImageView
                android:id="@+id/id_b32_icon_1"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginStart="20dp"
		    	android:focusable="false"
    			android:clickable="false"
                android:layout_marginEnd="20dp" />

            <com.accu_chek.solo_m.rcapp.application.gui.ui.FontManager
                android:id="@+id/id_b32_value1"
                style="@style/st_font_Value"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
		    	android:focusable="false"
	    		android:clickable="false"
                android:gravity="center" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:layout_marginStart="3dp"
            android:orientation="vertical"
	    	android:focusable="false"
    		android:clickable="false"
            android:paddingTop="3dp" >

            <ImageView
                android:id="@+id/id_b32_icon_2"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_gravity="center"
	    		android:focusable="false"
    			android:clickable="false"
                android:maxWidth="20dp" />

            <com.accu_chek.solo_m.rcapp.application.gui.ui.FontManager
                android:id="@+id/id_b32_value2"
                style="@style/st_font_Value"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
	    		android:focusable="false"
    			android:clickable="false"
                android:gravity="center" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:layout_marginStart="3dp"
            android:orientation="vertical"
            android:focusable="false"
    		android:clickable="false"
            android:paddingTop="3dp" >

            <ImageView
                android:id="@+id/id_b32_icon_3"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginStart="20dp"
		    	android:focusable="false"
	    		android:clickable="false"
                android:layout_marginEnd="20dp" />

            <com.accu_chek.solo_m.rcapp.application.gui.ui.FontManager
                android:id="@+id/id_b32_value3"
                style="@style/st_font_Value"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
		    	android:focusable="false"
	    		android:clickable="false"
                android:gravity="center" />
        </LinearLayout>
    </LinearLayout>

</FrameLayout>