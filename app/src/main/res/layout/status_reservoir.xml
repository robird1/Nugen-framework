<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:customfont="http://schemas.android.com/apk/res-auto"
    android:id="@+id/id_status_res"
    android:layout_width="match_parent"
    android:layout_height="51dp"
    android:fitsSystemWindows="true"
    android:minWidth="320dp" >

    <ImageView
        android:id="@+id/id_status_res_img"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="9dp"
        android:contentDescription="@string/cd_insulin_volume_icon"
        android:src="@drawable/reservoir_100u_58x51px" />

    <com.accu_chek.solo_m.rcapp.application.gui.ui.FontManager
        android:id="@+id/id_status_res_txt"
        style="@style/st_font_Status_Details"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginStart="71dp"
        android:text="150U"
        android:textColor="@color/main_grey" />



</RelativeLayout>
