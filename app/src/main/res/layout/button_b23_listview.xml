<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:clickable="false"
    android:focusable="false" >

    <LinearLayout
        android:id="@+id/id_button_b23_seperate"
        style="@style/st_button_vertical_default_margin_16px"
        android:layout_height="26dp"
        android:background="@color/light_brown"
    	android:focusable="false"
    	android:clickable="false">

        <com.accu_chek.solo_m.rcapp.application.gui.ui.FontManager
            android:id="@+id/id_b28_text"
            style="@style/st_font_SmallTitle_bold"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp"
            android:clickable="false"
            android:contentDescription="@string/cd_label1"
            android:focusable="false"
            android:gravity="center_vertical"
            android:lines="1"
            android:text="Forgot text?"
            android:textColor="@color/main_grey" />

    </LinearLayout>
    
    <LinearLayout
        android:id="@+id/id_button_b23_main"
        style="@style/st_button_b23"
        android:layout_width="match_parent"
    	android:focusable="false"
    	android:clickable="false"
         >

        <LinearLayout
            android:layout_width="102dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
    		android:focusable="false"
    		android:clickable="false"
             >

            <com.accu_chek.solo_m.rcapp.application.gui.ui.FontManager
                android:id="@+id/id_b23_title"
                style="@style/st_font_SmallTitle"
                android:layout_width="86dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:clickable="false"
                android:contentDescription="@string/cd_time"
                android:focusable="false"
                android:textColor="@color/main_grey" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:layout_marginEnd="8dp"
                android:layout_marginStart="8dp"
                android:orientation="horizontal"
    			android:focusable="false"
    			android:clickable="false"
                 >

                <ImageView
                    android:id="@+id/id_b23_icon_1_1"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:clickable="false"
                    android:contentDescription="@string/cd_healthevent"
                    android:focusable="false"
                    android:maxHeight="20dp"
                    android:maxWidth="20dp"
                    android:minHeight="20dp"
                    android:minWidth="20dp" />

                <ImageView
                    android:id="@+id/id_b23_icon_1_2"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginStart="2dp"
                    android:clickable="false"
                    android:contentDescription="@string/cd_mealtime"
                    android:focusable="false"
                    android:maxHeight="20dp"
                    android:maxWidth="20dp"
                    android:minHeight="20dp"
                    android:minWidth="20dp" />

                <ImageView
                    android:id="@+id/id_b23_icon_1_3"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginStart="2dp"
                    android:clickable="false"
                    android:contentDescription="@string/cd_note"
                    android:focusable="false" />

                <ImageView
                    android:id="@+id/id_b23_icon_1_4"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginStart="2dp"
                    android:clickable="false"
                    android:contentDescription="@string/cd_basal"
                    android:focusable="false"
                    android:maxHeight="20dp"
                    android:maxWidth="20dp"
                    android:minHeight="20dp"
                    android:minWidth="20dp" />

            </LinearLayout>
        </LinearLayout>

        <ImageView
            android:layout_width="2dp"
            android:layout_height="match_parent"
            android:src="@drawable/button23_separator"
    		android:focusable="false"
    		android:clickable="false"
             />

        <LinearLayout
            android:layout_width="60dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
    		android:focusable="false"
    		android:clickable="false"
             >

            <ImageView
                android:id="@+id/id_b23_icon_2"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:clickable="false"
                android:focusable="false"
                android:maxHeight="20dp"
                android:maxWidth="20dp"
                android:minHeight="20dp"
                android:minWidth="20dp" />

            <com.accu_chek.solo_m.rcapp.application.gui.ui.FontManager
                android:id="@+id/id_b23_value1"
                style="@style/st_font_Value"
                android:layout_width="42dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:clickable="false"
                android:contentDescription="@string/cd_bg_value"
                android:focusable="false"
                android:gravity="center"
                android:textColor="@color/main_grey" />

        </LinearLayout>

        <ImageView
            android:layout_width="2dp"
            android:layout_height="match_parent"
            android:src="@drawable/button23_separator"
    		android:focusable="false"
    		android:clickable="false"
            />

        <LinearLayout
            android:layout_width="60dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
    		android:focusable="false"
    		android:clickable="false"
             >

            <LinearLayout
                android:id="@+id/id_b23_icon_3"
                android:layout_width="42dp"
                android:layout_height="20dp"
                android:layout_marginStart="9dp"
                android:orientation="horizontal"
    			android:focusable="false"
    			android:clickable="false"
                 >

                <ImageView
                    android:id="@+id/id_b23_icon_3_1"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:clickable="false"
                    android:contentDescription="@string/cd_icon2"
                    android:focusable="false"
                    android:maxHeight="20dp"
                    android:maxWidth="20dp"
                    android:minHeight="20dp"
                    android:minWidth="20dp" />

                <ImageView
                    android:id="@+id/id_b23_icon_3_2"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:clickable="false"
                    android:contentDescription="@string/cd_icon3"
                    android:focusable="false"
                    android:maxHeight="20dp"
                    android:maxWidth="20dp"
                    android:minHeight="20dp"
                    android:minWidth="20dp"
                    android:paddingStart="2dp" />

            </LinearLayout>

            <com.accu_chek.solo_m.rcapp.application.gui.ui.FontManager
                android:id="@+id/id_b23_value2"
                style="@style/st_font_Value"
                android:layout_width="42dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:clickable="false"
                android:contentDescription="@string/cd_insulinvalue"
                android:focusable="false"
                android:textColor="@color/main_grey" />

        </LinearLayout>

        <ImageView
            android:layout_width="2dp"
            android:layout_height="match_parent"
            android:src="@drawable/button23_separator"
    		android:focusable="false"
    		android:clickable="false"
             />

        <LinearLayout
            android:layout_width="60dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
    		android:focusable="false"
    		android:clickable="false"
             >

            <ImageView
                android:id="@+id/id_b23_icon_4"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:clickable="false"
                android:contentDescription="@string/cd_icon4"
                android:focusable="false"
                android:maxHeight="20dp"
                android:maxWidth="20dp"
                android:minHeight="20dp"
                android:minWidth="20dp" />

            <com.accu_chek.solo_m.rcapp.application.gui.ui.FontManager
                android:id="@+id/id_b23_value3"
                style="@style/st_font_Value"
                android:layout_width="42dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:clickable="false"
                android:contentDescription="@string/cd_carbvalue"
                android:focusable="false"
                android:gravity="center"
                android:textColor="@color/main_grey" />

        </LinearLayout>
    </LinearLayout>

</FrameLayout>