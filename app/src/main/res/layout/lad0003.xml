<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/background"
    android:gravity="center_horizontal"
    android:orientation="vertical" >

    <!-- <include layout="@layout/picker7" /> -->

    <LinearLayout
        android:id="@+id/lad003_picker"
        android:layout_width="@dimen/dim_picker_width"
        android:layout_height="@dimen/dim_picker_height"
        android:orientation="vertical" >
    </LinearLayout>

    <LinearLayout
        android:id="@+id/lad003_lower_ab"
        android:layout_width="@dimen/dim_lower_ab_width"
        android:layout_height="@dimen/dim_lower_ab_height"
        android:layout_alignParentBottom="true"
        android:orientation="vertical" >
    </LinearLayout>

</RelativeLayout>