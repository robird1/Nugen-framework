<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:customfont="http://schemas.android.com/apk/res-auto"
    android:id="@+id/warning_relativeLayout1"
    android:layout_width="@dimen/dim_warning_bg_width"
    android:layout_height="@dimen/dim_warning_bg_height"
    android:background="@color/info"
    android:orientation="vertical" >

    <TextView
        android:layout_width="@dimen/dim_warning_bg_width"
        android:layout_height="@dimen/dim_warning_bg_height"
        android:background="@drawable/background_1blue" />

    <include
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="31dp"
        layout="@layout/warning_title" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:layout_alignParentStart="true"
        android:layout_marginEnd="38dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="134dp"
        android:orientation="vertical" >

        <com.accu_chek.solo_m.rcapp.application.gui.ui.FontManager
            android:id="@+id/warning_text"
            style="@style/st_font_Default"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxLines="6"
            android:textColor="@color/main_grey" />
    </LinearLayout>

    <RelativeLayout
        android:id="@+id/warning_rel1_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_marginEnd="38dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="278dp"
        android:orientation="horizontal" >

        <com.accu_chek.solo_m.rcapp.application.gui.ui.FontManager
            android:id="@+id/warning_text_low_left_1"
            style="@style/st_font_Default"
            android:layout_width="142dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:contentDescription="@string/cd_label1"
            android:maxLines="1"
            android:textColor="@color/main_grey" />

        <com.accu_chek.solo_m.rcapp.application.gui.ui.FontManager
            android:id="@+id/warning_text_low_right_1"
            style="@style/st_font_Second_Line"
            android:layout_width="96dp"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_gravity="right"
            android:contentDescription="@string/cd_uptext"
            android:gravity="end"
            android:maxLines="1"
            android:textColor="@color/main_grey" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_marginEnd="38dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="300dp"
        android:orientation="horizontal" >

        <com.accu_chek.solo_m.rcapp.application.gui.ui.FontManager
            android:id="@+id/warning_text_low_left_2"
            style="@style/st_font_Default"
            android:layout_width="142dp"
            android:layout_height="wrap_content"
            android:contentDescription="@string/cd_Label2"
            android:maxLines="1"
            android:textColor="@color/main_grey" />

        <com.accu_chek.solo_m.rcapp.application.gui.ui.FontManager
            android:id="@+id/warning_text_low_right_2"
            style="@style/st_font_Second_Line"
            android:layout_width="96dp"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:contentDescription="@string/cd_downtext"
            android:gravity="right"
            android:maxLines="1"
            android:textColor="@color/main_grey" />

    </RelativeLayout>

    <LinearLayout
        android:id="@+id/lad001d_lower_ab"
        android:layout_width="280dp"
        android:layout_height="48dp"
        android:layout_centerVertical="false"
        android:layout_marginStart="20dp"
        android:layout_marginTop="337dp"
        android:orientation="vertical" >
    </LinearLayout>

</RelativeLayout>