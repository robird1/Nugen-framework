<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="320dp"
    android:layout_height="405dp"
    android:background="@color/background"
    android:gravity="center_horizontal"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/id_lad17ab_top"
        android:layout_width="320dp"
        android:layout_height="100dp"
        android:layout_marginBottom="6dp"
        android:layout_marginTop="14dp"
        android:gravity="center_horizontal"
        android:minHeight="100dp"
        android:minWidth="320dp" />

    <LinearLayout
        android:layout_width="320dp"
        android:layout_height="405dp"
        android:layout_below="@+id/id_lad17ab_top"
        android:orientation="vertical" >
        
    <ScrollView
        android:id="@+id/lad0017ab_scroll"
        android:layout_width="320dp"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_weight="1">

        <TableLayout
            android:id="@+id/lad0017ab_tableLayout"
            android:layout_width="@dimen/dim_button_width_b1_b17"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/id_lad17ab_top"
            android:gravity="center_horizontal" >
        </TableLayout>
    </ScrollView>

    <LinearLayout
        android:id="@+id/lad0017ab_lower_ab"
        android:layout_width="@dimen/dim_lower_ab_width"
        android:layout_height="@dimen/dim_lower_ab_height"
        android:layout_alignParentBottom="true"
        android:orientation="vertical" >
    </LinearLayout>
    </LinearLayout>

</RelativeLayout>