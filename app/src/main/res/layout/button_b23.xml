<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" >

    <LinearLayout
        android:id="@+id/id_button_b23"
        style="@style/st_button_b23"
        android:layout_width="match_parent" >

        <LinearLayout
            android:layout_width="102dp"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <com.accu_chek.solo_m.rcapp.application.gui.ui.FontManager
                android:id="@+id/id_b23_title"
                style="@style/st_font_SmallTitle"
                android:layout_width="86dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"                 android:textColor="@color/main_grey" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:layout_marginEnd="8dp"
                android:layout_marginStart="8dp"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/id_b23_icon_1_1"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:maxHeight="20dp"
                    android:maxWidth="20dp"
                    android:minHeight="20dp"
                    android:minWidth="20dp" />

                <ImageView
                    android:id="@+id/id_b23_icon_1_2"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginStart="2dp"
                    android:maxHeight="20dp"
                    android:maxWidth="20dp"
                    android:minHeight="20dp"
                    android:minWidth="20dp" />

                <ImageView
                    android:id="@+id/id_b23_icon_1_3"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginStart="2dp" />

                <ImageView
                    android:id="@+id/id_b23_icon_1_4"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginStart="2dp"
                    android:maxHeight="20dp"
                    android:maxWidth="20dp"
                    android:minHeight="20dp"
                    android:minWidth="20dp" />
            </LinearLayout>
        </LinearLayout>

        <ImageView
            android:layout_width="2dp"
            android:layout_height="match_parent"
            android:src="@drawable/button23_separator" />

        <LinearLayout
            android:layout_width="60dp"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/id_b23_icon_2"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:maxHeight="20dp"
                android:maxWidth="20dp"
                android:minHeight="20dp"
                android:minWidth="20dp" />

            <com.accu_chek.solo_m.rcapp.application.gui.ui.FontManager
                android:id="@+id/id_b23_value1"
                style="@style/st_font_Value"
                android:layout_width="42dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:gravity="center"                 android:textColor="@color/main_grey"/>
        </LinearLayout>

        <ImageView
            android:layout_width="2dp"
            android:layout_height="match_parent"
            android:src="@drawable/button23_separator" />

        <LinearLayout
            android:layout_width="60dp"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/id_b23_icon_3"
                android:layout_width="42dp"
                android:layout_height="20dp"
                android:layout_marginStart="9dp"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/id_b23_icon_3_1"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:maxHeight="20dp"
                    android:maxWidth="20dp"
                    android:minHeight="20dp"
                    android:minWidth="20dp" />

                <ImageView
                    android:id="@+id/id_b23_icon_3_2"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:maxHeight="20dp"
                    android:maxWidth="20dp"
                    android:minHeight="20dp"
                    android:minWidth="20dp"
                    android:paddingStart="2dp" />
            </LinearLayout>

            <com.accu_chek.solo_m.rcapp.application.gui.ui.FontManager
                android:id="@+id/id_b23_value2"
                style="@style/st_font_Value"
                android:layout_width="42dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"                 android:textColor="@color/main_grey" />
        </LinearLayout>

        <ImageView
            android:layout_width="2dp"
            android:layout_height="match_parent"
            android:src="@drawable/button23_separator" />

        <LinearLayout
            android:layout_width="60dp"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/id_b23_icon_4"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:maxHeight="20dp"
                android:maxWidth="20dp"
                android:minHeight="20dp"
                android:minWidth="20dp" />

            <com.accu_chek.solo_m.rcapp.application.gui.ui.FontManager
                android:id="@+id/id_b23_value3"
                style="@style/st_font_Value"
                android:layout_width="42dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:gravity="center"                 android:textColor="@color/main_grey" />
        </LinearLayout>
    </LinearLayout>

</FrameLayout>